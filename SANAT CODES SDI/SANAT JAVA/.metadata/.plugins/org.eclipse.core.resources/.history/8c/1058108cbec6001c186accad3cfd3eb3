package Oops_classes;
class Vehicles{
	String veh_name;
	String veh_typ;
	int veh_whelr;
	String veh_color;
	
	Vehicles(){
		System.out.println("This is an example of color eassy");
		Vehicles v1 = new Vehicles();// This is known as the constructor of obj v1
		v1.veh_name = "Mersidese Gwagon";
		v1.veh_typ = "Racing";
		v1.veh_whelr = 4;
		v1.veh_color = "blue";
	}
	public void Printveh() {
		System.out.println(this.veh_typ);
		System.out.println(this.veh_whelr);
		System.out.println(this.veh_color);
	}
}
public class NonparamerParameterConstructor {
	public static void main(String [] args) {
		
		v1.Printveh();
	}
}

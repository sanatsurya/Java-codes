package Oops_classes;
class Add{
	int a;
	int b;
	public void AddNum() {
		System.out.println(this.a + this.b);
	}
	public void SubNum() {

		if(a<b)
			System.out.println(this.b - this.a);
		else
			System.out.println(this.a - this.b);
	}
	public void MultNum() {
		System.out.println(a*b);
		
	}
	public void DivNum() {
		if (a<b) {
			System.out.println(this.b / this.a);
		}
		else
			System.out.println(this.a/ this.b);
	}
}
public class AdditionSubstraction {
	public static void main(String[] args) {
		Add eqn1 = new Add();
		eqn1.a = 8;
		eqn1.b = 9;
		eqn1.AddNum();
		
		
	}
}
